{
  "environment": "production",
  "base_domain": "vimbisopay.africa",
  "subdomain": "vimbiso-chatserver",
  "aws_region": "af-south-1",
  "vpc_cidr": "10.0.0.0/16",
  "ecs_task": {
    "cpu": "1024",
    "memory": "2048"
  },
  "az_count": 2,
  "autoscaling": {
    "min_capacity": 1,
    "max_capacity": 4,
    "cpu_threshold": 80,
    "memory_threshold": 80
  },
  "django_env": {
    "debug": true,
    "django_log_level": "INFO",
    "app_log_level": "INFO"
  }
}
